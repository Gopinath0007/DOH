//     }
}

.mx-scrollcontainer-middle {
    background:  #f6f5fa;
    // background-image: linear-gradient(310deg, rgb(255 188 132), rgb(168 152 251));
}

.c-newlayout{
     .region-topbar {
        position: relative;
        z-index: 2;
        min-height: 48px;
        background: white;
        box-shadow: 0px 0px 6px #dedede;
    }
  .region-topbar .topbar-content {
        align-items: center;
        min-height: 48px;
        justify-content: center;
        display: block;
    }
    .toggle-btn img{
        width: 35px;
    height: auto;
    margin-top: 25px;
    }
    .region-sidebar {
        background-color: #ffffff;
    }
    .mx-navigationtree .navbar-inner > ul > li > a {
        background-color: #ffffff;
    }
    .region-sidebar .mx-navigationtree .navbar-inner > ul > li a{
        font-weight: 600;
    }
    .region-sidebar .mx-navigationtree .navbar-inner > ul > li > a:hover {
        background-color: #f8fafb;
    color: #8a00eb;
    font-weight: 600;
    }
    .region-sidebar .mx-navigationtree .navbar-inner > ul > li a.active {
        background-color: rgb(224 230 234);
    color: #8a00eb;
    font-weight: 600;
       
    }
    a:focus {
        outline: none;
    }
    .mx-scrollcontainer-wrapper .navbar-inner .glyphicon:hover, .glyphicon:active {
        background-color: #e4ecf0;
        margin: 5px;
        width: 42px !important;
        height: 42px !important;
        color: #91b6bb;
        /* box-shadow: 0px 0px 0px #e3e3e3; */
    }
    .mx-scrollcontainer-wrapper .navbar-inner .glyphicon {
        color: #666666;
        margin: 5px;
        width: 42px !important;
        height: 42px !important;
    }
    .region-sidebar {
     
        z-index: 101;
        box-shadow: 0 0 0px rgb(0 0 0 / 14%), 2px 3px 6px rgb(192 192 192 / 46%);
    }
}
.cham{
    display: flex;
}
.userbg {
    background: #e4edf2;
    margin: 15px 8px;
    border-top-left-radius: 40px;
    border-bottom-left-radius: 40px;
    padding: 5px;
    border-radius: 40px;
    // box-shadow: 1px 0px 5px 0px #848484;
  }

  .userimg{
    border: 4px solid rgb(210 210 210);
    border-radius: 50px;
    width: 60px;
    margin-left: 36px;
  }

  .c-chart1{
    padding: 30px 30px 18px;
    display: flex;
    justify-content: center;
    box-shadow: 0px 0px 18px 0px #cecece33;
    margin-top: 0px;
    min-height: 330px;
    border-radius: 15px;
    background: white;
  }
  .mx-dataview-controls {
   
    border-top:none;
  }

  .c-gname .mx-text{


    color: #9829b9;
  }

  //db cards

  .c-sla{
    background-color: rgb(255 200 88 / 18%);
    padding: 12px;
    border-radius: 25px;
    color: white;
font-weight: 500;
  }
  .c-pending{
    background-color: rgb(66 75 239 / 17%);
    padding: 12px;
    border-radius: 25px;
    color: white;
font-weight: 500;
width: auto !important;
  }
  .c-progress{
    background-color:rgb(239 66 66 / 26%);
    padding: 12px;
    border-radius: 25px;
    color: white;
font-weight: 500;
  }
  .c-approved{
    background-color:rgb(87 239 66 / 19%);
    padding: 12px;
    border-radius: 25px;
    color: white;
font-weight: 500;
  }
  .s-btn,.mx-grid-search-button, .mx-grid-reset-button{
    background: linear-gradient(308deg, #f25974 0%, #bea0d6 85%);
    color: white;
    border-radius: 20px;
    min-width: 110px;
    margin: 0px 10px;
    border-width: 0;
  }
  .s-btn:hover,.mx-grid-search-button:hover, .mx-grid-reset-button:hover{
    background: linear-gradient(308deg, #f25974 0%, #bea0d6 85%);
    color: white;
    border-radius: 20px;
    min-width: 110px;
   
    border-width: 0;
  }

  .c-tb{
    box-shadow: 0px 0px 18px 0px #cecece33;
    background: white;
    margin: 25px 0px;
    padding:  15px;
    border-radius: 20px;
  }

  .mx-datagrid table th, .mx-datagrid table th, .mx-datagrid table tbody tr td {
    
    border-bottom-width: 0;
   
}
.mx-grid-searchbar{
    padding: 20px 5px;
    background: #f3f3f3;
    border-radius: 15px;
}
