.doh2 .mx-navigationlist-item{
    display: flex;
    flex-direction: row;
    align-items: center;
}

.mx-navigationlist-item:focus-visible {
    outline: none !important;
}
.c-nav .mx-navigationlist-item {
    padding: 27px !important;
}
.c-usernav{
    display: flex;
    justify-content: center;
    margin-top: 15px;
    align-items: center;
//     img{
